<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane minHeight="800.0" minWidth="700.0" style="-fx-background-color: #FFC999;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.button_cli">
   <children>
      <TextField fx:id="cli_txt" layoutX="14.0" layoutY="732.0" prefHeight="47.0" prefWidth="557.0" style="-fx-background-radius: 7px;" />
      <TextArea fx:id="cli_chat" editable="false" layoutX="7.0" layoutY="158.0" prefHeight="554.0" prefWidth="687.0" style="-fx-cursor: hand;" wrapText="true">
         <font>
            <Font size="18.0" />
         </font>
      </TextArea>
      <Button fx:id="cli_send" layoutX="580.0" layoutY="732.0" mnemonicParsing="false" onAction="#Send_cli" prefHeight="47.0" prefWidth="113.0" style="-fx-background-radius: 8px;" text="Send" />
      <Label layoutX="197.0" layoutY="14.0" prefHeight="47.0" prefWidth="252.0" text="      Client Chat Box">
         <font>
            <Font name="System Bold Italic" size="24.0" />
         </font>
      </Label>
      <TextField fx:id="cli_ip" layoutX="181.0" layoutY="99.0" promptText="Enter IP Address" style="-fx-background-radius: 4px;" />
      <Label layoutX="7.0" layoutY="94.0" prefHeight="40.0" prefWidth="164.0" text="IP Address :">
         <font>
            <Font name="Berlin Sans FB Demi Bold" size="24.0" />
         </font>
      </Label>
      <Button fx:id="cli_ip_conn" layoutX="402.0" layoutY="99.0" mnemonicParsing="false" onAction="#Connect_cli" prefHeight="31.0" prefWidth="91.0" style="-fx-background-radius: 5px;" text="Connect" />
   </children>
</AnchorPane>
